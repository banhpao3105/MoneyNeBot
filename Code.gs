    } else if (data === 'edit_transaction' || data.startsWith('edit_transaction_')) {
      // X·ª≠ l√Ω ch·ªânh s·ª≠a giao d·ªãch
      Logger.log("DEBUG: edit_transaction callback received for user: " + chatId);
