    // Validate amount
    if (!amountStr.match(/^\d+$/)) {
      sendText(chatId, "‚ùå S·ªë ti·ªÅn kh√¥ng h·ª£p l·ªá! Ch·ªâ nh·∫≠p s·ªë, v√≠ d·ª•: 25000");
